<sdf version="1.5">
  <model name="human" xmlns:xacro="http://www.ros.org/wiki/xacro">
   <xacro:property name="armEffort" value="0" /> <!-- Standard value is 30 -->
    <xacro:property name="wristEffort" value="0" /> <!-- Standard value is 10 -->
    <xacro:include filename="../pr2/pr2.include" />
    <!-- Controller for the pr2 arm -->
    <!-- <plugin name="pr2_arm_controller" filename="libStableControllerPlugin.dylib">    </plugin>-->



    <!-- Human -->
    <!-- TODO: These should not be needed but parameters aren't working -->
    <xacro:property name="xoffset" value="0.939280" />
    <xacro:property name="yoffset" value="-0.25" />
    <xacro:property name="zoffset" value="0" />
    <xacro:property name="prefix" value="" />
    <xacro:include filename="../human/human.include"/>
    
    <!-- Virtual connecting joint -->
    <joint name="virtual_robot_human_connection" type="ball">
      <parent>r_wrist_roll_link</parent>
      <child>${prefix}left_thigh</child>
      <pose>0 0 0.145 0.0 0.0 0.0</pose><!-- Connect at point at robot hand height on thigh -->
      <axis> 
        <xyz>0.0 1 0.0</xyz>
      </axis>
      <axis2>
        <xyz>0.0 0.0 1.0</xyz>
      </axis2>
      <physics>
        <provide_feedback>1</provide_feedback>
        <ode>
          <provide_feedback>1</provide_feedback>
          <erp>0.3</erp>
        </ode>
      </physics>
    </joint>
  </model>
</sdf>
